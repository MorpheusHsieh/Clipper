SET TALK OFF
STORE 0 TO I,R,F1,F2,C
STORE SPACE(8) TO DATE,DATE1,DATE2,DATE3,DATE4
DATE1='900701'
DATE2='900731'
DATE3=VAL(DATE1)
DATE4=VAL(DATE2)
USE TEMP
R=RECCOUNT()
I=0
DO WHILE I<R
   I=I+1
   DATE=VAL(SUBSTR(DTOC(ONUDATE),7,2)+SUBSTR(DTOC(ONUDATE),1,2)+SUBSTR(DTOC(ONUDATE),4,2))
   F1=DATE>DATE3
   F2=DATE<DATE4
   @ 10,5  SAY DATE
   @ 10,20 SAY DATE3
   @ 10,35 SAY DATE4
   @ 10,50 SAY F1 
   @ 10,55 SAY F2
   C=0
   DO WHILE C=0 
      C=INKEY()
   ENDDO
   DELETE FOR DATE<DATE3.OR.DATE>DATE4 
   GO I                        
ENDDO
SET TALK ON
